{
  "name": "aweb/aweb-framework",
  "description": "The Simple PHP Framework.",
  "keywords": ["framework", "aweb"],
  "license": "Apache 2.0",
  "type": "project",
  "require": {
    "zircote/swagger-php": "2.0.0",
    "nikic/fast-route": "^1.3",
    "catfan/Medoo": "^1.5",
    "monolog/monolog": "^1.23"
